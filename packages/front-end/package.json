{
  "name": "front-end",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.1.9",
    "@tiernebre/kecleon": "^1.33.0",
    "@types/jest": "^26.0.24",
    "@types/node": "^16.3.2",
    "@types/react": "^17.0.14",
    "@types/react-dom": "^17.0.9",
    "jsonwebtoken": "^8.5.1",
    "node-sass": "^6.0.0",
    "prettier": "^2.3.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.11.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "react-switch": "^6.0.0",
    "react-use-localstorage": "^3.5.3",
    "react-use-websocket": "^2.7.1",
    "rooks": "^5.0.3",
    "typescript": "^4.3.5",
    "web-vitals": "^2.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "compile": "react-scripts build",
    "test:unit": "react-scripts test --watchAll=false",
    "test:watch": "react-scripts test",
    "test": "yarn test:unit",
    "lint": "yarn lint:style && eslint . --max-warnings=0",
    "lint:style": "prettier --check .",
    "eject": "react-scripts eject",
    "format": "prettier --write ."
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended",
      "plugin:@typescript-eslint/recommended-requiring-type-checking",
      "react-app",
      "react-app/jest",
      "plugin:testing-library/react",
      "plugin:jsx-a11y/recommended",
      "prettier"
    ],
    "plugins": [
      "testing-library",
      "jest-dom"
    ],
    "parserOptions": {
      "project": [
        "tsconfig.json"
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^8.5.4",
    "@types/mockdate": "^3.0.0",
    "@types/react-router-dom": "^5.1.8",
    "eslint-plugin-jest-dom": "^3.9.0",
    "eslint-plugin-testing-library": "^4.9.0",
    "fetch-mock-jest": "^1.5.1",
    "mockdate": "^3.0.5",
    "testdouble": "^3.16.1"
  },
  "jest": {
    "transform": {
      "^.+\\.[t|j]sx?$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!@react-use-localstorage)/"
    ]
  }
}
