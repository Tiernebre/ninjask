    "testdouble": "^3.16.1"
  },
  "jest": {
    "transform": {
      "^.+\\.[t|j]sx?$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!@react-use-localstorage)/"
    ]
  }
